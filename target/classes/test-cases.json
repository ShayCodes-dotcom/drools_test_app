{
  "testCases": [

    {
      "testName": "TC-01: Complete valid applicant - all rules pass",
      "description": "All fields populated with valid values. Every rule should fire and pass.",
      "input": {
        "applicant": {
          "applicantFirstName": "Jane",
          "applicantLastName": "Doe",
          "applicantDateOfBirth": "1990-05-15",
          "applicantSocialSecurityNumber": "123-45-6789",
          "applicantAnnualIncome": 75000,
          "applicantEmploymentStatus": "Full-Time",
          "applicantEmployerName": "Acme Corp",
          "applicantStreetAddress": "123 Main Street",
          "applicantCity": "Baton Rouge",
          "applicantState": "LA",
          "applicantZipCode": "70801",
          "applicantAge": 34,
          "applicantCreditScore": 750,
          "applicantDriversLicenseNumber": "DL-9876543",
          "applicantPassportNumber": null,
          "applicantStateIdNumber": null
        }
      },
      "expected": {
        "fullyValid": true,
        "applicantDataIsValid": true,
        "applicantIncomeIsVerified": true,
        "applicantAddressIsComplete": true,
        "applicantIsEligible": true,
        "applicantIdentityIsConfirmed": true
      }
    },

    {
      "testName": "TC-02: Empty applicant - no rules pass",
      "description": "All fields null/absent. No rules should fire.",
      "input": {
        "applicant": {}
      },
      "expected": {
        "fullyValid": false,
        "applicantDataIsValid": false,
        "applicantIncomeIsVerified": false,
        "applicantAddressIsComplete": false,
        "applicantIsEligible": false,
        "applicantIdentityIsConfirmed": false
      }
    },

    {
      "testName": "TC-03: Personal data only",
      "description": "Only personal identity fields filled in. Only Rule 1 should pass.",
      "input": {
        "applicant": {
          "applicantFirstName": "John",
          "applicantLastName": "Smith",
          "applicantDateOfBirth": "1985-12-01",
          "applicantSocialSecurityNumber": "987-65-4321"
        }
      },
      "expected": {
        "fullyValid": false,
        "applicantDataIsValid": true,
        "applicantIncomeIsVerified": false,
        "applicantAddressIsComplete": false,
        "applicantIsEligible": false,
        "applicantIdentityIsConfirmed": false
      }
    },

    {
      "testName": "TC-04: Missing first name",
      "description": "All personal data present except firstName. Rule 1 should fail.",
